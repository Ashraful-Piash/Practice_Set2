package com.piash;

import java.util.Arrays;

public class Array {

	public static void main(String[] args) {
		int [] numbers = new int[5];
		numbers [0] = 1;
		numbers [1] = 2;
		numbers [2] = 7;
		numbers [3] = 5;
		
		Arrays.sort(numbers);
		System.out.println(Arrays.toString(numbers));
		System.out.println(numbers.length);
		
		int x = 2;
		int y = x++;
		System.out.println(y);
	}
}