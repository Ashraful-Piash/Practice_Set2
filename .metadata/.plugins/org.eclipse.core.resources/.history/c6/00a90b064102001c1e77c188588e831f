package math;

import java.util.Scanner;

public class Hello {

	public static void main (String[]args) {
		Scanner s = new Scanner (System.in);
		System.out.println("enter number");
		int a = s.nextInt();
		System.out.println("enter another number");
		int b = s.nextInt();
		
		int result = a+b;
		System.out.println(result);
	}

}
